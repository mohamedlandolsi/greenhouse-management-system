# =============================================================================
# .dockerignore for Spring Boot services
# Optimizes Docker build context by excluding unnecessary files
# =============================================================================

# Build output
target/
build/
*.class
*.jar
*.war
*.ear

# IDE files
.idea/
*.iml
*.ipr
*.iws
.vscode/
*.swp
*.swo
.settings/
.project
.classpath

# Git
.git/
.gitignore
.gitattributes

# Documentation
*.md
LICENSE
docs/

# Docker files (we don't need them in context)
Dockerfile*
docker-compose*
.dockerignore

# Logs
logs/
*.log

# Test files
src/test/

# Maven
.mvn/
mvnw
mvnw.cmd

# OS files
.DS_Store
Thumbs.db

# CI/CD
.github/
.gitlab-ci.yml
Jenkinsfile
.circleci/

# Environment files
.env
.env.*
*.env

# Temporary files
tmp/
temp/
*.tmp
*.bak
